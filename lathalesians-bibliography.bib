@Article{edalat-1999,
  Title                    = {Semi-pullbacks and bisumulation in categories of Markov processes},
  Author                   = {Abbas Edalat},
  Journal                  = {Mathematical Structures in Computer Science},
  Year                     = {1999},
  Pages                    = {523--543},
  Volume                   = {9},

  Abstract                 = {We show that the category whose objects are families of Markov processes on Polish spaces, with a given transition kernel, and whose morphisms are transition probability preserving, surjective continuous maps has semi-pullbacks, i.e., for any pair of morphisms $f_i : S_i \rightarrow S_$ ($i = 1, 2$), there exists an object $V$ and morphisms $\pi_i: V \rightarrow S_i$ ($i = 1, 2$) such that $f_1 \circ \pi_1 = f_2 \circ \pi_2$. This property holds for various full subcategories, including that of families of Markov processes on locally compact second countable spaces, and in the larger category where the objects are families of Markov processes on analytic spaces and morphisms are transition probability preserving surjective Borel maps. It also follows that the category of probability measures on Polish spaces and measure-preserving continuous surjective maps has semi-pullbacks. A main consequence of our result is that probabilistic bisimulation for labelled Markov processes on all these categories, defined in terms of spans of morphisms, is an equivalence relation.},
  Owner                    = {Paul Bilokon},
  Timestamp                = {2009.09.30}
}

